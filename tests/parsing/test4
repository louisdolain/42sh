 ((ls -l ; echo Hello) | cat -e) | grep lib | grep test  in: -1, out: -1

--  (ls -l ; echo Hello) | cat -e   in: 0, out: 1

----   ls -l ; echo Hello   in: 0, out: 1

------   ls -l  in: 0, out: 1

------ echo Hello   in: 0, out: 1

---- cat -e   in: 0, out: 1

-- grep lib | grep test  in: 0, out: 1

---- grep lib  in: 0, out: 1

---- grep test  in: 0, out: 1

